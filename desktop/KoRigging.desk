root {
    split 0.342396 V {
        tabs 0 1 {
            pane panetab1 -a 0 -l 10 -H /obj/ -T OBJ -m neteditor
            pane panetab2 -a 0 -l 10 -H /obj/ -T OBJ -m chaneditor
