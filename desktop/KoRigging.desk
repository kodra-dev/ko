            pane panetab8 -a 0 -l 0 -H /tasks/ -T TOPNET -m neteditor
        }
        split 0.5 H R {
            tabs 0 1 {
                pane panetab6 -a 0 -l 10 -H /obj/ -T OBJ -m details
            }
            tabs 1 1 {
                pane panetab7 -a 0 -l 10 -H /obj/ -T OBJ -m pythonpanel
                pane panetab5 -a 0 -l 10 -H /obj/ -T OBJ -m pythonpanel
            }
        }
    }

    panel panel2 -w 2630 611 1270 785 {
        tabs 0 1 {
            pane FloatingPanel -a 0 -l 10 -H /obj/ -T OBJ -m sceneview
        }
    }
}
